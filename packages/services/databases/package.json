{
  "name": "@project/databases",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./db1/*": "./src/generated/db1/*",
    "./db1": {
      "types": "./src/generated/db1/index.d.ts",
      "import": "./src/generated/db1/index.mjs",
      "require": "./src/generated/db1/index.js"
    },
    "./db2/*": "./src/generated/db2/*",
    "./db2": {
      "types": "./src/generated/db2/index.d.ts",
      "import": "./src/generated/db2/index.mjs",
      "require": "./src/generated/db2/index.js"
    }
  },
  "main": "src/index.ts",
  "scripts": {
    "build:prisma": "pnpm prisma:push",
    "prisma:validate": "lnv vault -d -- tsx scripts/validate.ts",
    "prisma:generate": "tsx scripts/generate.ts",
    "prisma:migrate": "lnv vault -d -- tsx scripts/migrate.ts",
    "prisma:format": "lnv vault -d -- tsx scripts/format.ts",
    "prisma:studio": "lnv vault -d -- tsx scripts/studio.ts",
    "prisma:pull": "lnv vault -d -- tsx scripts/pull.ts",
    "prisma:push": "lnv vault -d -- tsx scripts/push.ts",
    "postinstall": "pnpm build:prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "bignumber.js": "^9.1.2",
    "chalk": "^5.4.1",
    "execa": "^9.5.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "fs-extra": "^11.3.0",
    "prisma": "^6.5.0"
  }
}
